    gentoo: dev-libs/libsigc++
    fedora: libsigc++-devel
    arch,manjarolinux: libsigc++
    opensuse: libsigc++2-devel
    macos-brew: libsigc++

rbtree: gem

msgpack-ruby: 
    gem : msgpack

websocket-gem:
    gem: websocket

binding_of_caller:
    gem: binding_of_caller

activesupport:
    gem: activesupport

eigen3:
    debian, ubuntu: libeigen3-dev
    fedora: eigen3-devel
    arch,manjarolinux: eigen3
    macos-brew: eigen
    opensuse: eigen3-devel

pkg-config:
    debian,ubuntu:
        default: pkg-config
    fedora,opensuse: pkg-config
    arch,manjarolinux: pkg-config
    macos-brew: pkg-config

